{
  "name": "rack-overflow",
  "version": "1.0.0",
  "description": "What if Stack Overflow was for individuals looking to work out?",
  "main": "index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "backend-install": "npm install --prefix backend",
    "backend": "npm run dev --prefix backend",
    "backend-seed": "npm run seed --prefix backend",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "frontend-build": "npm run build --prefix frontend",
    "build": "npm run backend-install && npm run frontend-install && npm run frontend-build && npm run backend-seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AErdenberger/Rack-Overflow.git"
  },
